# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

GET     /                           controllers.HomeController.index

POST	/login						controllers.HomeController.login

GET		/welcome					controllers.HomeController.welcome

POST    /token                      controllers.HomeController.mockAuthServerGetToken

GET     /customers                  controllers.HomeController.getCustomers

GET     /users                      controllers.HomeController.getUsers

GET     /roles                      controllers.HomeController.getRoles

GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
